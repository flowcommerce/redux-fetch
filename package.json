{
  "name": "@flowio/redux-fetch",
  "version": "0.11.1",
  "description": "Universal data fetching bindings for React, React Router, and Redux",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rm -rf ./lib ./coverage",
    "build": "babel ./src --out-dir ./lib",
    "coverage": "istanbul cover --report html _mocha -- ./test/specs",
    "lint": "eslint --cache --fix ./src ./test",
    "test": "mocha ./test/specs",
    "prebuild": "npm-run-all clean coverage",
    "precoverage": "npm run lint",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "watch:build": "nodemon --exec 'npm run build' --watch './src' --watch './test'",
    "watch:test": "nodemon --exec 'mocha --debug ./test/specs' --watch './src' --watch './test'",
    "postpublish": "greenkeeper-postpublish"
  },
  "keywords": [
    "fetching",
    "react",
    "redux",
    "react-router"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flowcommerce/redux-fetch.git"
  },
  "author": "Flow Commerce",
  "contributors": [
    "Christian Mu√±oz <christian.munoz@flow.io>"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-register": "^6.9.0",
    "hoist-non-react-statics": "^1.2.0",
    "invariant": "^2.2.1",
    "react-static-container": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-router": "^2.0.0",
    "redux": "^3.0.0"
  },
  "devDependencies": {
    "@flowio/babel-preset-flowio": "^1.0.2",
    "@flowio/eslint-config-flowio": "^3.0.0",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "enzyme": "^2.3.0",
    "eslint": "^3.2.2",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "greenkeeper-postpublish": "^1.0.0",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "^9.2.1",
    "mocha": "^3.0.1",
    "nodemon": "^1.9.2",
    "npm-run-all": "^2.3.0",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.6.0",
    "redux": "^3.5.2",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0"
  }
}
