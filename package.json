{
  "name": "@flowio/redux-fetch",
  "version": "0.13.0",
  "description": "Universal data fetching bindings for React, React Router, and Redux",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rm -rf ./lib ./coverage",
    "build": "babel ./src --out-dir ./lib",
    "coverage": "istanbul cover --report html _mocha -- ./test/specs",
    "lint": "eslint --cache --ext .jsx,.js .",
    "test": "mocha ./test/specs",
    "prebuild": "npm-run-all clean coverage",
    "precoverage": "npm run lint",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "watch:build": "nodemon --exec 'npm run build' --watch './src' --watch './test'",
    "watch:test": "nodemon --exec 'mocha --debug ./test/specs' --watch './src' --watch './test'",
    "postpublish": "greenkeeper-postpublish"
  },
  "keywords": [
    "fetching",
    "react",
    "redux",
    "react-router"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flowcommerce/redux-fetch.git"
  },
  "author": "Flow Commerce",
  "contributors": [
    "Christian Mu√±oz <christian.munoz@flow.io>"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-register": "^6.18.0",
    "hoist-non-react-statics": "^1.2.0",
    "invariant": "^2.2.2",
    "react-static-container": "^1.0.1"
  },
  "peerDependencies": {
    "prop-types": "^15.5.8",
    "react": "^15.0.0",
    "react-router": "^3.0.0",
    "redux": "^3.0.0"
  },
  "devDependencies": {
    "@flowio/babel-preset-flowio": "1.1.0",
    "@flowio/eslint-config-flowio": "7.0.0",
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "enzyme": "2.6.0",
    "eslint": "3.12.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "greenkeeper-postpublish": "1.0.1",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "9.9.1",
    "mocha": "3.2.0",
    "nodemon": "1.11.0",
    "npm-run-all": "3.1.2",
    "prop-types": "15.5.8",
    "react": "15.4.1",
    "react-addons-test-utils": "15.4.1",
    "react-dom": "15.4.1",
    "react-router": "3.0.0",
    "redux": "3.6.0",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0"
  }
}
