{
  "name": "@flowio/redux-fetch",
  "version": "0.15.0-beta.1",
  "description": "Universal data fetching bindings for React, React Router, and Redux",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rm -rf ./lib ./coverage",
    "build": "babel ./src --out-dir ./lib",
    "lint": "eslint --cache --ext .jsx,.js .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test nyc mocha test/specs/**/*.{js,jsx}",
    "prepublish": "npm-run-all clean lint test build",
    "watch:build": "babel --watch ./src --out-dir ./lib",
    "watch:test": "mocha --watch --watch-extensions js,jsx test/specs/**/*.{js,jsx}",
    "postpublish": "greenkeeper-postpublish"
  },
  "keywords": [
    "fetching",
    "isomorphic",
    "universal",
    "react",
    "redux",
    "react-router"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flowcommerce/redux-fetch.git"
  },
  "author": "Flow Commerce",
  "contributors": [
    "Christian Mu√±oz <christian.munoz@flow.io>"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "hoist-non-react-statics": "^2.3.1"
  },
  "peerDependencies": {
    "prop-types": "^15.0.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^5.0.0",
    "react-router": "^4.0.0",
    "react-router-config": "^1.0.0-beta.4",
    "redux": "^3.0.0",
    "redux-thunk": "^2.0.0"
  },
  "devDependencies": {
    "@flowio/babel-preset-flowio": "^1.4.1",
    "@flowio/eslint-config-flowio": "^11.0.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.1.1",
    "deep-freeze": "^0.0.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.8.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "greenkeeper-postpublish": "^1.3.0",
    "jsdom": "^11.5.1",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.3.0",
    "redux-thunk": "^2.2.0",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0"
  }
}
